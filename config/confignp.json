{
    "repetitions": 10,
    "testData": "testdata.json",
    "benchmark": {
        "encoders": {
            "converter": "\\Benchmark\\Converters\\ArrayConverter",
			"formats": {
				"native": {
					"libs": [
						{
							"name": "PHP serialize",
							"class": "\\Benchmark\\Encoders\\Native\\PhpSerializeFunction",
							"url": "http://php.net/manual/en/function.serialize.php"
						}
					]
				},
				"json": {
					"libs": [
						{
							"name": "PHP json_encode",
							"class": "\\Benchmark\\Encoders\\Json\\PhpJsonEncodeFunction",
							"url": "http://php.net/manual/en/function.json-encode.php"
						},
						{
							"name": "webmozart/json",
							"class": "\\Benchmark\\Encoders\\Json\\WebmozartJson",
							"url": "https://github.com/webmozart/json",
							"version": "1.2.2"
						}
					]
				},
				"xml": {
					"libs": [
						{
							"name": "openlss/lib-array2xml",
							"class": "\\Benchmark\\Encoders\\Xml\\OpenlssLibArray2Xml",
							"url": "https://github.com/nullivex/lib-array2xml",
							"version": "0.5.0"
						},
						{
							"name": "spatie/array-to-xml",
							"class": "\\Benchmark\\Encoders\\Xml\\SpatieArrayToXml",
							"url": "https://github.com/spatie/array-to-xml",
							"version": "2.2.0"
						},
						{
							"name": "fillup/array2xml",
							"class": "\\Benchmark\\Encoders\\Xml\\FillupArray2Xml",
							"url": "https://github.com/fillup/array2xml",
							"version": "0.5.1"
						}
					]
				},
				"msgpack": {
					"libs": [
						{
							"name": "msgpack/msgpack-php",
							"class": "\\Benchmark\\Encoders\\Xml\\MsgpackOfficial",
							"url": "https://github.com/msgpack/msgpack-php",
							"version": "2.0.1"
						},
						{
							"name": "rybakit/msgpack",
							"class": "\\Benchmark\\Encoders\\Xml\\RybakitMsgpack",
							"url": "https://github.com/rybakit/msgpack.php",
							"version": "0.2.1"
						}
					]
				}
			}
        },
		"decoders": {
			"formats": {
				"native": {
					"converter": "\\Benchmark\\Converters\\SerializeConverter",
					"libs": [
						{
							"name": "PHP unserialize",
							"class": "\\Benchmark\\Decoders\\Native\\PhpUnserializeFunction",
							"url": "http://php.net/manual/en/function.unserialize.php"
						}
					]
				},
				"json": {
					"converter": "\\Benchmark\\Converters\\JsonConverter",
					"libs": [
						{
							"name": "PHP json_decode",
							"class": "\\Benchmark\\Decoders\\Json\\PhpJsonDecodeFunction",
							"url": "http://php.net/manual/en/function.json-decode.php"
						},
						{
							"name": "webmozart/json",
							"class": "\\Benchmark\\Decoders\\Json\\WebmozartJson",
							"url": "https://github.com/webmozart/json",
							"version": "1.2.2"
						}
					]
				},
				"xml": {
					"converter": "\\Benchmark\\Converters\\XmlConverter",
					"libs": [
						{
							"name": "openlss/lib-array2xml",
							"class": "\\Benchmark\\Decoders\\Xml\\OpenlssLibArray2Xml",
							"url": "https://github.com/nullivex/lib-array2xml",
							"version": "0.5.0"
						}
					]
				},
				"msgpack": {
					"converter": "\\Benchmark\\Converters\\MsgpackConverter",
					"libs": [
						{
							"name": "msgpack/msgpack-php",
							"class": "\\Benchmark\\Decoders\\Xml\\MsgpackOfficial",
							"url": "https://github.com/msgpack/msgpack-php",
							"version": "2.0.1"
						},
						{
							"name": "rybakit/msgpack",
							"class": "\\Benchmark\\Decoders\\Xml\\RybakitMsgpack",
							"url": "https://github.com/rybakit/msgpack.php",
							"version": "0.2.1"
						}
					]
				}
			}
		}
    }
}